{
  "name": "react-tictactoe",
  "version": "1.1.0",
  "description": "React + Redux: Tic Tac Toe",
  "author": "August “Kai” Kaiser <augustkaiser@gmail.com>",
  "repository": "awkaiser/react-tictactoe",
  "homepage": "https://github.com/awkaiser/react-tictactoe",
  "license": "MIT",
  "private": true,
  "keywords": [
    "tic tac toe",
    "react",
    "minimax"
  ],
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.3.0"
  },
  "main": "src/app.js",
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "~3.19.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-loader": "^1.9.0",
    "express": "^4.15.4",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-rc.3",
    "standard": "^10.0.3",
    "tape": "^4.6.3",
    "webpack": "^3.5.5"
  },
  "scripts": {
    "build": "./node_modules/webpack/bin/webpack.js",
    "dev": "nodemon -e js,pug --watch server server/index.js",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.json 'src/**/*.js*'",
    "postinstall": "npm run build",
    "start": "node server/index.js",
    "test": "./node_modules/.bin/tape -r babel-register test.js",
    "watch": "npm run build -- -w"
  }
}
